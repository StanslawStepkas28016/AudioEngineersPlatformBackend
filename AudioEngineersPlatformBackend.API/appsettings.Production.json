    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/log.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:o} [{Level:u3}] ({ClientIp}/{RequestId}/{TraceId}) {Message}{NewLine}{Exception}"
        }
      },
      {
        "Name": "OpenTelemetry"
      }
    ]
  },
  "AllowedHosts": "*"
